{
  "expo": {
    "name": "Where Is My Car",
    "description": "A mobile app for finding parking and finding my car",
    "githubUrl": "https://github.com/lobobabysaurus/where-is-my-car",
    "slug": "where-is-my-car",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.tubbycatgames.whereismycar",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"        
      ]
    },
    "ios": {
      "supportsTablet": true
    }
  }
}
